{
  "name": "quickercater",
  "version": "1.0.0",
  "description": "A catering app for caterers to cater.",
  "main": "server.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "engines": {
    "node": "6.6.0"
  },
  "scripts": {
    "start": "nodemon server/server.js --ignore client/",
    "build": "browserify -t [ babelify --presets [ react ] ] client/main.js -o build/bundle.js",
    "test": "./scripts/test_client.sh && ./scripts/test_server.sh",
    "test_client": "./scripts/test_client.sh",
    "test_server": "./scripts/test_server.sh",
    "lint": "./scripts/lint.sh"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/five-square/caterchimp.git"
  },
  "author": "Francis Toto, Walter Ayala, Abel Costilla, Vinod Thimmapuram, Garrett Holmes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/five-square/caterchimp/issues"
  },
  "homepage": "https://github.com/five-square/caterchimp#readme",
  "dependencies": {
    "authport": "^1.0.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.0",
    "browserify-middleware": "^7.0.0",
    "chai": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.15.4",
    "neo4j": "^2.0.0-RC2",
    "nodemailer": "^0.7.1",
    "nodemon": "^1.10.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-color": "^2.3.2",
    "react-cropper": "^0.9.4",
    "react-dom": "^15.3.2",
    "react-sortable": "^1.1.0",
    "react-swipe": "^5.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "supertest-as-promised": "^4.0.0",
    "swipe-js-iso": "^2.0.3"
  },
  "devDependencies": {
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.0.2",
    "pre-commit": "^1.1.3",
    "supertest": "^2.0.0"
  }
}
